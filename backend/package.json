{
  "name": "backend",
  "version": "1.0.0",
  "description": "Proyecto 2 Cifrados",
  "main": "bin/www.js",
  "type": "module",
  "scripts": {
    "start": "node bin/www.js",
    "dev": "nodemon bin/www.js",
    "test": "jest",
    "coverage": "jest --coverage"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": ["<rootDir>/jest.setup.js"],
    "collectCoverageFrom": [
      "apiServices/**/*.js",
      "utils/**/*.js"
    ],
    "coverageDirectory": "coverage",
    "coverageReporters": ["lcov", "text"],
    "testMatch": ["**/__tests__/**/*.[jt]s"],
    "transform": {
      "^.+\\.js$": "babel-jest"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pabloozamora/uvg-cifrados-proyecto2.git"
  },
  "keywords": [
    "cifrados"
  ],
  "author": "Pablo Zamora, Erick Guerra, Diego Aquino",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/pabloozamora/uvg-cifrados-proyecto2/issues"
  },
  "homepage": "https://github.com/pabloozamora/uvg-cifrados-proyecto2#readme",
  "dependencies": {
    "argon2": "^0.43.0",
    "crypto-js": "^4.2.0",
    "dotenv": "^16.5.0",
    "elliptic": "^6.6.1",
    "express": "^5.1.0",
    "google-auth-library": "^9.15.1",
    "helmet": "^8.1.0",
    "js-sha256": "^0.11.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.14.1",
    "nodemon": "^3.1.10",
    "qrcode": "^1.5.4",
    "socket.io": "^4.8.1",
    "speakeasy": "^2.0.0"
  },
  "devDependencies": {
    "@sonar/scan": "^4.3.2",
    "@types/jest": "^30.0.0",
    "jest": "^30.2.0",
    "jest-environment-node": "^30.2.0",
    "supertest": "^7.1.4",
    "@babel/core": "^7.22.0",
    "@babel/preset-env": "^7.22.0",
    "babel-jest": "^30.2.0"
  }
}
